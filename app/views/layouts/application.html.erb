<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= javascript_include_tag 'http://code.jquery.com/jquery-1.6.min.js' %>
  <%= javascript_include_tag 'rails', 'application'%>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
</head>
<body>
<div class="container">
      <%= render 'layouts/header' %>
       <section class="round">
        <% flash.each do |key, value| %>
            <div class="flash <%= key %>"><%= value %></div>
        <% end %>
        <%= yield %>
      </section>
</div>
<%= debug(params) if Rails.env.development? %>
</body>
</html>
